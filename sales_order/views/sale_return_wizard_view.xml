  <odoo>
    <record id="view_return_wizard_form" model="ir.ui.view">
        <field name="name">return.wizard.form</field>
        <field name="model">return.wizard</field>
        <field name="arch" type="xml">
            <form string="إرجاع المنتجات" create="false" edit="false">
                <group>
                    <field name="user_id" readonly="1"/>
                    <field name="date_return"/>
                    <field name="sale_order_id" readonly="1"/>
                    <field name="return_reason"/>
                </group>

                <group>
                    <field name="return_line_ids">
                        <tree editable="bottom">
                            <field name="product_id" readonly="1"/>
                            <field name="original_qty" readonly="1"/>
                            <field name="return_qty"/>
<!--                            <field name="onhand_qty"/>-->
                        </tree>
                    </field>
                </group>

                <footer>
                    <button name="action_submit_return" type="object" string="Submit" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_return_wizard" model="ir.actions.act_window">
        <field name="name">Return Wizard</field>
        <field name="res_model">return.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>
</odoo>